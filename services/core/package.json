{
  "name": "@bierbot/core",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "cdk bootstrap --profile bierbot-developer --qualifier de91078a5b",
    "bootstrap-production": "cdk bootstrap --context stage=production --qualifier de91078a5b",
    "bootstrap-staging": "cdk bootstrap --context stage=staging --qualifier de91078a5b",
    "deploy": "cdk deploy --profile bierbot-developer",
    "deploy-production": "cdk deploy --context stage=production",
    "deploy-staging": "cdk deploy --context stage=staging",
    "destroy": "cdk destroy --profile bierbot-developer",
    "destroy-production": "cdk destroy --context stage=production",
    "destroy-staging": "cdk destroy --context stage=staging",
    "format-check": "prettier --check . ../../.prettierignore",
    "format-fix": "prettier --write . ../../.prettierignore",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "nx run format-check && nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.341.0",
    "@aws-sdk/client-scheduler": "^3.341.0",
    "@aws-sdk/lib-dynamodb": "^3.341.0",
    "@bierbot/serverless-configuration": "workspace:^",
    "@middy/core": "^4.4.3",
    "@middy/http-urlencode-body-parser": "^4.4.3",
    "@slack/web-api": "^6.8.1",
    "@swarmion/serverless-contracts": "^0.28.2",
    "@swarmion/serverless-helpers": "^0.28.2",
    "ajv": "^8.12.0",
    "dynamodb-toolbox": "^0.8.5",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@vitest/coverage-c8": "0.30.0",
    "aws-cdk": "^2.81.0",
    "aws-cdk-lib": "^2.81.0",
    "constructs": "^10.2.35",
    "dependency-cruiser": "^13.0.2",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "0.30.0"
  }
}
